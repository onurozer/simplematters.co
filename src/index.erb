layout: default
---
<section class="container max-w-2xl mb-6">
  <div class="">
    <a class="block w-24 mx-auto " href="/">
      <img src="<%= relative_url '/images/logo.svg' %>" alt="<%= "#{site.metadata.title} Logo" %>" />
    </a>
  </div>
  <div class="">
    <div class="flex flex-col items-center">
      <p class="p-4"><%= site.metadata.tagline %></p>
      <a class="inline-block px-4 py-2 text-xs font-bold text-gray-700 uppercase border border-gray-500 rounded-sm" href="<%= relative_url '/about' %>">Learn more</a>
    </div>
  </div>
</section>
<%= render "articles"%>
